	"title": "should fetch user profiles",

	"operationName": "Get User Profiles by Email Id",

  "method": "GET",
  "uri": "/user-profile/users",

  "users": {
    "invokingUser": {
      "username": "[[$CCD_CASEWORKER_AUTOTEST_EMAIL]]",
      "password": "[[$CCD_CASEWORKER_AUTOTEST_PASSWORD]]"
    }
  },

	"specs": [
		"has the 200 OK code"
	],

	"request": {
		"headers": {
			"ServiceAuthorization": "[[DEFAULT_AUTO_VALUE]]",
			"Content-Type": "application/json",
      "email-ids-users-to-find": "{{CCD_CASEWORKER_AUTOTEST_EMAIL}}"
		}
	},

	"expectedResponse": {
