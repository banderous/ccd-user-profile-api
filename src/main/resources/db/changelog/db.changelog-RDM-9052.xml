<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    
    <changeSet id="RDM-9052-1" author="henry">
	    <preConditions onFail="MARK_RAN">
			<sqlCheck expectedResult="0">SELECT count(c.relname) FROM pg_class c WHERE c.relkind = 'S' and c.relname = 'user_profile_audit_id_seq'</sqlCheck>
	    </preConditions>
		<addAutoIncrement columnDataType="int" columnName="id" tableName="user_profile_audit"/>  
	</changeSet>
</databaseChangeLog>
