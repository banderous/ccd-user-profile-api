<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    
    <changeSet id="RDM-9052-1" author="henry">
	    <createSequence sequenceName="user_profile_audit_id_seq" startValue="1" incrementBy="1"/>     
	</changeSet>
    <changeSet id="RDM-9052-2" author="henry">
		<sql dbms="postgresql">select setval('user_profile_audit_id_seq', max(id)+1) from user_profile_audit</sql> 
    </changeSet>
    <changeSet id="RDM-9052-3" author="henry">
		<sql dbms="postgresql">Alter SEQUENCE user_profile_audit_id_seq increment by 50</sql> 
    </changeSet>
</databaseChangeLog>
